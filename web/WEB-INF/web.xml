<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlet.Login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/servlet/Login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>addServlet</servlet-name>
        <servlet-class>servlet.addServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>addServlet</servlet-name>
        <url-pattern>/servlet/addServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Present</servlet-name>
        <servlet-class>servlet.Present</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Present</servlet-name>
        <url-pattern>/servlet/Present</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>searchRentInfo</servlet-name>
        <servlet-class>servlet.searchRentInfo</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>searchRentInfo</servlet-name>
        <url-pattern>/servlet/searchRentInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>searchDelayStu</servlet-name>
        <servlet-class>servlet.searchDelayStu</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>searchDelayStu</servlet-name>
        <url-pattern>/servlet/searchDelayStu</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>searchStuinfoInHall</servlet-name>
        <servlet-class>servlet.searchStuinfoInHall</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>searchStuinfoInHall</servlet-name>
        <url-pattern>/servlet/searchStuinfoInHall</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>searchAdvisor</servlet-name>
        <servlet-class>servlet.searchAdvisor</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>searchAdvisor</servlet-name>
        <url-pattern>/servlet/searchAdvisor</url-pattern>
    </servlet-mapping>

</web-app>